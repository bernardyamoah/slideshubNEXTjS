{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/empty/page.js", "lib/Empty.tsx", "lib/createCampus.ts", "pages/_app.tsx", "customHooks/useCampuses.js", "customHooks/useCourse.js", "customHooks/usePrograms.js", "app/api/route/getUploadedBy.js", "customHooks/useCourses.js", "components/course-edit.jsx", "app/(clients)/campus/[campusId]/programs/[programId]/[...courseId]/page.jsx", "app/(clients)/campus/[campusId]/programs/page.jsx", "components/fileUpload.jsx", "components/AddSlides.jsx"],
  "exclude": ["node_modules"]
}
